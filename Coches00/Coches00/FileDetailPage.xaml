        <StackLayout Padding="10">
            <Image Source="{Binding Archivo}"/>
            <Button x:Name="BtnCerrar" Text="Cerrar" Clicked="BtnCerrar_Clicked"/>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>